<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hpp.helloandroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Test1Activity"
            android:launchMode="singleInstance"
            android:label="@string/activity_name_test1"/>
        <activity android:name=".Test2Activity"
            android:label="@string/activity_name_test2"/>
        <activity android:name=".Test3DlgActivity"
            android:label="Test3DlgActivity"
            android:theme="@style/Theme.AppCompat.Dialog"/>
    </application>

</manifest>